{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "public/",
    "sourceMap": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
